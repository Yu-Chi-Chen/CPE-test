#include <stdio.h>

int main() {
	long long int num1,num2;
	while (scanf("%lld %lld", &num1,&num2) ==2) {
		if (num1 > num2) {
			printf("%lld\n", num1 - num2);
		}
		else {
			printf("%lld\n", num2 - num1);
		}
	}
	return 0;
}

//要注意大小
/*scanf輸入有參考到下列網址:
    https://blog.csdn.net/weixin_42212753/article/details/104147590
*/
//數學函式庫的abs()返回值是int
